# Depot Configuration
# Copy this file to .env and customize values

# InfluxDB settings
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=changeme-influxdb-password
INFLUXDB_ORG=muni
INFLUXDB_BUCKET=bvr
# Generate a secure token: openssl rand -hex 32
INFLUXDB_ADMIN_TOKEN=changeme-generate-secure-token

# Grafana settings
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=changeme-grafana-password

# Session storage path (must exist on host)
# This is where rovers upload .rrd session files via SFTP
SESSIONS_PATH=/data/bvr-sessions

# Retention period in days (used by cleanup script)
RETENTION_DAYS=30
