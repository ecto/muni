{
  "framework": "nextjs",
  "buildCommand": "curl -L -o /tmp/typst.tar.xz https://github.com/typst/typst/releases/download/v0.14.2/typst-x86_64-unknown-linux-gnu.tar.xz && tar -xJf /tmp/typst.tar.xz -C /tmp && export PATH=\"/tmp/typst-x86_64-unknown-linux-gnu:$PATH\" && make -C ../paper web-docs && npm run build",
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": true },
    { "source": "/about.html", "destination": "/about", "permanent": true },
    { "source": "/products.html", "destination": "/products", "permanent": true },
    { "source": "/docs.html", "destination": "/docs", "permanent": true },
    { "source": "/investors.html", "destination": "/investors", "permanent": true },
    { "source": "/viewer.html", "destination": "/viewer", "permanent": true },
    { "source": "/success.html", "destination": "/success", "permanent": true },
    { "source": "/cancel.html", "destination": "/cancel", "permanent": true },
    { "source": "/log.html", "destination": "/log", "permanent": true }
  ]
}
